{
    "name": "tombenevides/dump-linter",
    "description": "A custom rules to remove dump functions from PHP code",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Tombenevides\\DumpLinter\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Tom Benevides",
            "email": "tom@tonge.dev"
        }
    ],
    "require": {
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "require-dev": {
        "pestphp/pest": "^1.22"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
